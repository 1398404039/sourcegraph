gitserver: .bin/gitserver
indexer: .bin/indexer
searcher: .bin/searcher
github-proxy: .bin/github-proxy
lsp-proxy: .bin/lsp-proxy -prof-http=:6061
xlang-go: .bin/xlang-go -mode=tcp -addr=:4389 -prof-http=:6062
frontend: vendor/.bin/rego -installenv=GOGC=off,GODEBUG=sbrk=1 -tags="${GOTAGS-}" -extra-watches='app/templates/*' sourcegraph.com/sourcegraph/sourcegraph/cmd/frontend
web: sh -c "cd ./web && npm install && npm run watch -- --progress=false"
syntect_server: docker run --name=syntect_server --rm -p8000:80 -e QUIET='true' sourcegraph/syntect_server
