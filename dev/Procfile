gitserver: env SRC_PROF_HTTP=:6068 .bin/gitserver
indexer: .bin/indexer
query-runner: env SRC_PROF_HTTP=:6067 .bin/query-runner
repo-updater: .bin/repo-updater
searcher: env SRC_PROF_HTTP=:6069 $EXEC_SEARCHER .bin/searcher
symbols: env SRC_PROF_HTTP=:6070 .bin/symbols
github-proxy: .bin/github-proxy
lsp-proxy: env SRC_PROF_HTTP=:6061 .bin/lsp-proxy
xlang-go: .bin/xlang-go -mode=tcp -addr=:4389 -prof-http=:6062
frontend: env SRC_PROF_HTTP=:6060 $EXEC_FRONTEND .bin/frontend
watch-go: node_modules/.bin/chokidar --silent "**/*.go" "cmd/frontend/internal/graphqlbackend/schema.graphql" --ignore node_modules --ignore web -c "./dev/restart.sh"
web: sh -c "npm --prefix ./web install && npm --prefix ./web run watch"
syntect_server: ./dev/syntect_server
zoekt-indexserver: ./dev/zoekt-wrapper zoekt-indexserver -data_dir $HOME/.sourcegraph/zoekt -mirror_config ./dev/zoekt-mirror.json -mirror_duration 10m -fetch_interval 1m
zoekt-webserver: ./dev/zoekt-wrapper zoekt-webserver -index $HOME/.sourcegraph/zoekt/index -pprof -rpc
