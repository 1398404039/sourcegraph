gitserver: .bin/gitserver
indexer: .bin/indexer
query-runner: .bin/query-runner
repo-updater: .bin/repo-updater
searcher: $EXEC_SEARCHER .bin/searcher
symbols: .bin/symbols
github-proxy: .bin/github-proxy
lsp-proxy: .bin/lsp-proxy
xlang-go: .bin/xlang-go -mode=tcp -addr=:4389
frontend: $EXEC_FRONTEND .bin/frontend
watch: node_modules/.bin/chokidar --silent "**/*.go" "cmd/frontend/internal/graphqlbackend/schema.graphql" "schema/*.json" --ignore node_modules --ignore web -c "./dev/handle-change.sh {path}"
web: sh -c "npm --prefix ./web install && npm --prefix ./web run watch"
syntect_server: ./dev/syntect_server
zoekt-indexserver: ./dev/zoekt-wrapper .bin/zoekt-sourcegraph-indexserver -sourcegraph_url http://localhost:3090 -index $HOME/.sourcegraph/zoekt/index -interval 1m -listen :6072
zoekt-webserver: ./dev/zoekt-wrapper .bin/zoekt-webserver -index $HOME/.sourcegraph/zoekt/index -pprof -rpc
