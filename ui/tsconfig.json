{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
        "sourcegraph/*": ["web_modules/sourcegraph/*"],
        "vs/*": ["node_modules/vscode/src/vs/*"]
    },
    "lib": ["es2016", "dom"],
    "types": ["mocha", "lodash"],
    "jsx": "react",
    "relaySchema": "../api/schema.json",
    "sourceMap": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "scripts"
  ]
}
