{
  "compilerOptions": {
    "target": "es5", // don't generate "class" statements, etc.
    "module": "es6", // generate "import" statements which will be processed by webpack
    "moduleResolution": "node",
    "baseUrl": "web_modules",
    "jsx": "react",
    "strictNullChecks": true,
    "sourceMap": true,
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ]
}
