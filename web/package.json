{
  "name": "sourcegraph",
  "version": "0.0.1",
  "description": "Sourcegraph",
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcegraph/sourcegraph"
  },
  "engines": {
    "npm": "^6.0.0",
    "node": ">=8.0.0 <11.0.0"
  },
  "scripts": {
    "test": "mocha --require ts-node/register --require esm --require src/util/long-stack-traces.ts --timeout 10000 'src/**/*.test.ts?(x)'",
    "cover": "nyc --require ts-node/register --require esm mocha --timeout 10000 'src/**/*.test.ts?(x)'",
    "test-e2e": "mocha --require ts-node/register --require src/util/long-stack-traces.ts --timeout 60000 --slow 5000 --exit 'src/**/*.test.e2e.ts?(x)'",
    "build": "npm run schema && npm run graphql && npm run webpack",
    "watch": "concurrently --raw \"npm run watch-schema\" \"npm run graphql -- --watch\" \"npm run watch-webpack\"",
    "watch-webpack": "[ -n \"$USE_WEBPACK_DEV_SERVER\" ] && npm run webpack-dev-server || npm run webpack -- --watch",
    "webpack": "webpack-cli --config webpack.config.ts --colors",
    "webpack-dev-server": "webpack-dev-server --config webpack.config.ts --colors",
    "graphql": "ts-node introspect_schema",
    "schema": "export DEBUG=; mkdir -p src/schema && cd ../schema && json2ts -i settings.schema.json -o ../web/src/schema/settings.schema.d.ts && json2ts -i site.schema.json -o ../web/src/schema/site.schema.d.ts && cd ../web",
    "watch-schema": "chokidar --silent '../schema/**.schema.json' --initial -c 'npm run schema'",
    "lint": "npm run tslint && npm run stylelint",
    "tslint": "tslint -c tslint.json -p tsconfig.json -e '{node_modules/**,src/schema/**}' '**/*.ts?(x)'",
    "stylelint": "stylelint --quiet 'src/**/*.scss'",
    "stylelint-compact": "npm run stylelint -- --custom-formatter ./node_modules/stylelint-formatter-compact/index.js"
  },
  "nyc": {
    "include": [
      "src/**/*.ts?(x)"
    ],
    "exclude": [
      "**/*.test.ts?(x)"
    ],
    "extension": [
      ".tsx",
      ".ts"
    ]
  },
  "author": "John Rothfels <john@sourcegraph.com>",
  "license": "MIT",
  "devDependencies": {
    "@gql2ts/from-schema": "^1.7.2",
    "@sourcegraph/tsconfig": "^3.0.0",
    "@sourcegraph/tslint-config": "^11.1.1",
    "@types/babelify": "^7.3.5",
    "@types/bluebird": "^3.5.20",
    "@types/browserify": "^12.0.33",
    "@types/chai": "^4.1.2",
    "@types/chokidar": "^1.7.5",
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/d3-axis": "^1.0.8",
    "@types/d3-scale": "^1.0.10",
    "@types/d3-selection": "^1.2.0",
    "@types/d3-shape": "^1.2.1",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/escape-string-regexp": "0.0.31",
    "@types/extract-text-webpack-plugin": "^3.0.2",
    "@types/get-stream": "^3.0.1",
    "@types/graphql": "^0.11.7",
    "@types/highlight.js": "^9.1.10",
    "@types/js-base64": "^2.1.5",
    "@types/jsdom": "^11.0.4",
    "@types/lodash-es": "^4.17.0",
    "@types/marked": "^0.3.0",
    "@types/mocha": "^2.2.43",
    "@types/mz": "0.0.32",
    "@types/node": "^8.10.8",
    "@types/ora": "^1.3.1",
    "@types/p-retry": "^1.0.1",
    "@types/pretty-bytes": "^4.0.0",
    "@types/puppeteer": "^1.0.0",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-router": "^4.0.20",
    "@types/react-router-dom": "^4.2.3",
    "@types/strip-json-comments": "0.0.30",
