{
  "name": "sourcegraph",
  "version": "0.0.1",
  "description": "Sourcegraph",
  "engines": {
    "npm": "^5.3.0"
  },
  "scripts": {
    "test": "npm run build-tests && mocha --opts mocha.opts",
    "test-e2e": "npm run build-tests && mocha --opts mocha.opts.e2e",
    "build": "npm run graphql && npm run webpack",
    "watch": "concurrently --raw \"npm run watch-graphql\" \"npm run webpack -- --watch\"",
    "webpack": "webpack --config webpack.config.ts --profile --colors",
    "build-tests": "tsc -p ./tsconfig.test.json",
    "graphql": "cat ../cmd/frontend/internal/graphqlbackend/schema.graphql | gql2ts -o typings/graphqlschema.d.ts",
    "watch-graphql": "chokidar ../cmd/frontend/internal/graphqlbackend/schema.graphql --initial -c \"npm run graphql\"",
    "lint": "npm run tslint && npm run stylelint && npm run prettier",
    "tslint": "tslint -c tslint.json -p tsconfig.json -e 'node_modules/**' '**/*.ts?(x)'",
    "stylelint": "stylelint 'src/**/*.scss'",
    "prettier": "prettier '**/*.{js?(on),ts?(x)}' --write --list-different"
  },
  "author": "John Rothfels <john@sourcegraph.com>",
  "license": "MIT",
  "devDependencies": {
    "@sourcegraph/prettierrc": "^1.1.0",
    "@sourcegraph/tsconfig": "^2.0.0",
    "@sourcegraph/tslint": "^5.7.0",
    "@sourcegraph/tslint-config": "^7.0.0",
    "@types/auth0-js": "^8.6.3",
    "@types/escape-string-regexp": "0.0.31",
    "@types/extract-text-webpack-plugin": "^3.0.0",
    "@types/highlight.js": "^9.1.10",
    "@types/js-base64": "^2.1.5",
    "@types/jsdom": "^11.0.2",
    "@types/lodash": "^4.14.38",
    "@types/marked": "^0.0.28",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.28",
    "@types/react": "^15.6.6",
    "@types/react-dom": "^16.0.2",
    "@types/react-router": "^4.0.16",
    "@types/react-router-dom": "^4.0.8",
    "@types/strip-json-comments": "0.0.30",
    "@types/webpack": "^3.0.12",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-new-target": "^7.0.0-beta.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chokidar-cli": "^1.2.0",
    "chrome-launcher": "^0.8.1",
    "chromeless": "^1.3.0",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "gql2ts": "^1.3.0",
    "javascript-typescript-langserver": "^2.5.4",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.3",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.8.1",
    "sass-loader": "^6.0.6",
    "stylelint": "^8.1.1",
    "stylelint-config-prettier": "^1.0.2",
    "stylelint-config-recommended": "^1.0.0",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-language-service": "^0.9.6",
    "typescript": "^2.6.1",
    "typescript-formatter": "^6.0.0",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@sourcegraph/icons": "^1.9.0",
    "auth0-js": "^8.10.1",
    "bootstrap": "^4.0.0-beta",
    "copy-to-clipboard": "^3.0.7",
    "date-fns": "^2.0.0-alpha.7",
    "escape-string-regexp": "^1.0.5",
    "highlight.js": "^9.12.0",
    "immutable": "^3.8.1",
    "js-base64": "^2.3.2",
    "lodash": "^4.11.2",
    "marked": "^0.3.6",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-visibility-sensor": "^3.10.1",
    "rx-component": "^0.0.8",
    "rxjs": "^5.5.2",
    "strip-json-comments": "^2.0.1",
    "vscode-languageserver-types": "^3.4.0",
    "whatwg-url": "^6.2.1"
  }
}
