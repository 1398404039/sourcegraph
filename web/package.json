{
  "name": "sourcegraph",
  "version": "0.0.1",
  "description": "Sourcegraph",
  "scripts": {
    "test": "npm run build-tests && mocha --opts mocha.opts",
    "build": "webpack --config webpack.config.ts --progress --profile --colors",
    "build-tests": "tsc -p ./tsconfig.test.json",
    "watch": "npm run build -- --watch",
    "graphql": "get-graphql-schema http://localhost:3080/.api/graphql --json | gql2ts -o src/gqlschema.d.ts",
    "lint": "npm run tslint && npm run stylelint",
    "tslint": "tslint -c tslint.json -p tsconfig.json 'src/**/*.ts?(x)'",
    "stylelint": "stylelint 'src/**/*.scss'"
  },
  "author": "John Rothfels <john@sourcegraph.com>",
  "license": "MIT",
  "devDependencies": {
    "@sourcegraph/tsconfig": "^1.0.0",
    "@sourcegraph/tslint-config": "^2.0.0",
    "@types/auth0-js": "^8.6.3",
    "@types/escape-string-regexp": "0.0.31",
    "@types/extract-text-webpack-plugin": "^2.1.0",
    "@types/highlight.js": "^9.1.10",
    "@types/js-base64": "^2.1.5",
    "@types/jsdom": "^11.0.2",
    "@types/lodash": "^4.14.38",
    "@types/marked": "^0.0.28",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.28",
    "@types/react": "^16.0.7",
    "@types/react-dom": "^15.5.5",
    "@types/react-router": "^4.0.15",
    "@types/react-router-dom": "^4.0.8",
    "@types/webpack": "^3.0.12",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-new-target": "^7.0.0-beta.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "get-graphql-schema": "^2.1.1",
    "gql2ts": "^1.2.1",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.3",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "stylelint": "^8.1.1",
    "stylelint-config-recommended": "^1.0.0",
    "ts-loader": "^2.3.6",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-language-service": "^0.9.6",
    "tslint-webpack-plugin": "^0.5.0",
    "typescript": "^2.5.2",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@sourcegraph/icons": "^1.7.0",
    "auth0-js": "^8.10.1",
    "bootstrap": "^4.0.0-beta",
    "copy-to-clipboard": "^3.0.7",
    "date-fns": "^1.28.5",
    "escape-string-regexp": "^1.0.5",
    "highlight.js": "^9.12.0",
    "immutable": "^3.8.1",
    "js-base64": "^2.3.2",
    "lodash": "^4.11.2",
    "marked": "^0.3.6",
    "react": "^16.0.0",
